<?xml version='1.0' encoding='utf-8'?>
<widget android-versionCode="100017" id="com.precious.shri.manache.shlok" version="1.0.17" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <name>श्री मनाचे श्लोक</name>
    <description>
        Shri Manache Shlok : A Dialogue With Our Mind.
    </description>
    <author email="salunkecoep@gmail.com" href="http://cordova.io">
        Precious Technology
    </author>
    <content src="index.html" />
    <plugin name="cordova-plugin-whitelist" spec="1" />
    <access origin="*" />
    <allow-intent href="http://*/*" />
    <allow-intent href="https://*/*" />
    <allow-intent href="tel:*" />
    <allow-intent href="sms:*" />
    <allow-intent href="mailto:*" />
    <allow-intent href="geo:*" />
    <preference name="android-targetSdkVersion" value="29" />
    <platform name="android">
        <allow-intent href="market:*" />
        <icon density="ldpi" src="myicon/ldpi.png" />
        <icon density="mdpi" src="myicon/mdpi.png" />
        <icon density="hdpi" src="myicon/hdpi.png" />
        <icon density="xhdpi" src="myicon/xhdpi.png" />
        <icon density="xxhdpi" src="myicon/xxhdpi.png" />
        <icon density="xxxhdpi" src="myicon/xxxhdpi.png" />
        <splash src="myicon/splashscreen.png" />
    </platform>
    <platform name="ios">
        <allow-intent href="itms:*" />
        <allow-intent href="itms-apps:*" />
    </platform>
    <preference name="StatusBarBackgroundColor" value="#E65D15" />
    <preference name="ShowSplashScreenSpinner" value="true" />
    <preference name="SplashScreenSpinnerColor" value="#E65D15" />
    <preference name="AutoHideSplashScreen" value="true" />
    <preference name="FadeSplashScreen" value="false" />
    <preference name="FadeSplashScreenDuration" value="300" />
    <preference name="SplashShowOnlyFirstTime" value="false" />
    <preference name="SplashMaintainAspectRatio" value="true" />
    <preference name="SplashScreenDelay" value="5000" />
    <engine name="browser" spec="^6.0.0" />
    <engine name="android" spec="^7.1.4" />
</widget>
